{
  "manifest_version": 3,
  "name": "CleanTab",
  "version": "1.0",
  "description": "Minimal new tab page with weather",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
